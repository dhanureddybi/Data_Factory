{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "DhanuADF"
		},
		"AzureBlobInputData_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureBlobInputData'"
		},
		"DhanuAzureBlob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'DhanuAzureBlob'"
		},
		"OnPremDhanuSql_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremDhanuSql'",
			"defaultValue": "Integrated Security=True;Data Source=DESKTOP-F726TKR\\SQLEXPRESS;Initial Catalog=Movies"
		},
		"OnPremDhanuSql_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremDhanuSql'"
		},
		"OnPremDhanuSqlDbjoins_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'OnPremDhanuSqlDbjoins'",
			"defaultValue": "Integrated Security=True;Data Source=DESKTOP-F726TKR\\SQLEXPRESS;Initial Catalog=dbjoins"
		},
		"OnPremDhanuSqlDbjoins_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'OnPremDhanuSqlDbjoins'"
		},
		"AzureSqlDatabase_properties_typeProperties_connectionString_secretName": {
			"type": "string",
			"defaultValue": "DhanuSQLCon"
		},
		"AzureSqlKey_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://DhanuADFvault.vault.azure.net/"
		},
		"OnPremDhanuSql_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-F726TKR\\PC"
		},
		"OnPremDhanuSqlDbjoins_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "DESKTOP-F726TKR\\PC"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "DhanuADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureBlobInputData_connectionString": {
						"value": "[parameters('AzureBlobInputData_connectionString')]"
					},
					"DhanuAzureBlob_connectionString": {
						"value": "[parameters('DhanuAzureBlob_connectionString')]"
					},
					"OnPremDhanuSql_connectionString": {
						"value": "[parameters('OnPremDhanuSql_connectionString')]"
					},
					"OnPremDhanuSql_password": {
						"value": "[parameters('OnPremDhanuSql_password')]"
					},
					"OnPremDhanuSqlDbjoins_connectionString": {
						"value": "[parameters('OnPremDhanuSqlDbjoins_connectionString')]"
					},
					"OnPremDhanuSqlDbjoins_password": {
						"value": "[parameters('OnPremDhanuSqlDbjoins_password')]"
					},
					"AzureSqlKey_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureSqlKey_properties_typeProperties_baseUrl')]"
					},
					"AzureSqlDatabase_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('AzureSqlDatabase_properties_typeProperties_connectionString_secretName')]"
					},
					"OnPremDhanuSql_properties_typeProperties_userName": {
						"value": "[parameters('OnPremDhanuSql_properties_typeProperties_userName')]"
					},
					"OnPremDhanuSqlDbjoins_properties_typeProperties_userName": {
						"value": "[parameters('OnPremDhanuSqlDbjoins_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "DhanuADF_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'DhanuADF_ArmTemplate_0')]"
			]
		}
	]
}