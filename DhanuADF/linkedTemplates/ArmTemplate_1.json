												"TriggerName": {
													"value": "'@{pipeline().TriggerName}'",
													"type": "Expression"
												},
												"Log_File_Name": {
													"value": "'@{variables('Log_File_Name')}'",
													"type": "Expression"
												}
											},
											"datasetParameters": {
												"LogFile": {},
												"DesLogFile": {
													"des_file_name": {
														"value": "@variables('Log_File_Name')",
														"type": "Expression"
													}
												}
											}
										},
										"staging": {},
										"compute": {
