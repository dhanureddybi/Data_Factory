						],
						"userProperties": [],
						"typeProperties": {
							"expression": {
								"value": "@activity('CheckIfFileExists').output.exists",
								"type": "Expression"
							},
							"ifFalseActivities": [
								{
									"name": "Log_PipelineErrors",
									"type": "ExecuteDataFlow",
									"dependsOn": [],
